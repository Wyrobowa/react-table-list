{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "react/forbid-prop-types": [0, { "forbid": "any", "checkContextTypes": true, "checkChildContextTypes": true }],
    "import/prefer-default-export": "off"
  }
}

